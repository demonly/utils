{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "source": {
        "include": [
            "src",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
    },
    "opts": {
        "destination": "./doc/",
        "recurse": true,
        "tutorials": "tutorials",
        "template": "./node_modules/ink-docstrap/template"
    }
}